.QRadioButton {
    /* Radio Button Labels */
    color: #1d1b1c;
}

.QCheckBox {
    /* Checkbox Labels */
    color: #1d1b1c;
}

.QCheckBox:hover {}

.QValidatedLineEdit,
.QLineEdit,
.QTextEdit {
    /* Text Entry Fields */
    font-size: 12px;
    min-height: 25px;
    outline: 0;
    padding: 3px;
    border: 0px;
    background-color: #b8b0b5;
