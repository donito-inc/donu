}

.QPlainTextEdit {
    background-color: #b8b0b5;
}


QPushButton#warningIcon,#labelWalletStatus,#labelTransactionsStatus {
    background-color: transparent;
    border-style: none;
    padding-left: 25px;
    padding-right: 25px;
    padding-top: 1px;
    padding-bottom: 1px;
    margin: 0;
}

/***************************** Global Qt Objects ***********************************************************/

QPushButton {
    /* Global Button Style */
    background-color: #b8b0b5; /* For example on the settings dialog */
    border-width: 1px;
    border-style: outset;
    border-color: #1d1b1c;
    border-radius: 2px;
    color: #1d1b1c;
    font-size: 12px;
    font-weight: bold;
    padding-left: 25px;
    padding-right: 25px;
    padding-top: 1px;
    padding-bottom: 1px;
    height: 26px;
    margin: 2px;
}

QPushButton:hover {
    /* background-color: qlineargradient(y1: 0, y2: 1, stop: 0 #29333D, stop: 0.1 #29333D, stop: 0.101 #B6A27E, stop: 0.9 #B6A27E, stop: 0.901 #29333D, stop: 1 #29333D); */
    background-color: #1d1b1c;
    color: #ffffff;
}

QPushButton:focus {
    /* border:none; */
    /* outline:none; */
}

QPushButton:pressed {
    background-color: #1d1b1c;
    border: 1px solid #b8b0b5;
    /* background-color: qlineargradient(y1: 0, y2: 1, stop: 0 #29333D, stop: 0.1 #29333D, stop: 0.101 #B6A27E, stop: 0.9 #B6A27E, stop: 0.901 #29333D, stop: 1 #29333D); */
}


/* Special handling for statusbar-icons (which are partly QPushButtons) */

QPushButton#labelEncryptionIcon,
QPushButton#labelConnectionsIcon,
QPushButton#labelAutoMintIcon {
    border: 0px;
    padding-left: 0px;
    padding-right: 0px;
    padding-top: 0px;
    padding-bottom: 0px;
    margin: 0px;
}


/* QSpinBox::up-arrow:hover */


/* QSpinBox::down-arrow:hover */

QSpinBox {
    background-color: #b8b0b5;
    color: #1d1b1c;
}


QSpinBox::up-button:hover {
    background-color: #f2f2f2;
}

QSpinBox::down-button:hover {
    background-color: #f2f2f2;
}

QComboBox {
    /* Dropdown Menus */
    /* border: 1px solid #1d1b1c; */
    padding: 3px 5px 3px 5px;
    background-color: #b8b0b5;
    min-height: 25px;
    color: #1d1b1c;
}

QComboBox:checked {
    background-color: #b8b0b5;
}

QComboBox:editable {
    background-color: #b8b0b5;
    color: #1d1b1c;
    border: 0px solid transparent;
}

QComboBox::drop-down {
    width: 25px;
    border: 0px;
    background-color: #b8b0b5;
}

QComboBox::down-arrow {
    border-image: url(':/icons/downArrow') 0 0 0 0 stretch stretch;
}

QComboBox QListView {
    background-color: #b8b0b5;
    border: 1px solid #1d1b1c;
    padding-right: 1px;
    padding-left: 1px;
    color: #1d1b1c;
}

QComboBox QListView:hover {
    background-color: #000000;
}

QComboBox QAbstractItemView::item {
    margin: 4px;
}

QComboBox::item {
    background-color: #b8b0b5;
    color: #1d1b1c;
}

QComboBox::item:alternate {
    background: #b8b0b5;
}

QComboBox::item:selected {
    border: 0px solid transparent;
    background: #ffffff;
}

QComboBox::indicator {
    background-color: transparent;
    selection-background-color: transparent;
    color: transparent;
    selection-color: transparent;

}

QAbstractSpinBox {
    padding: 3px 5px 3px 5px;
    background: #29333D;
    font-size: 12px;
    min-height: 25px;
    outline: 0;
    padding: 3px;
    border: 0px;
    background-color: #b8b0b5;
    color: #1d1b1c;
    selection-color: #b8b0b5;
}

QAbstractSpinBox:!focus {
    background-color: #b8b0b5;
    color: #1d1b1c;
}

QAbstractSpinBox::up-button {
    subcontrol-origin: border;
    subcontrol-position: top right;
    width: 21px;
    background: #e7e7e7;
    /* Remove this to get 3D rendered buttons */
    padding-right: 1px;
    padding-left: 5px;
    padding-top: 2px;
}

QAbstractSpinBox::up-arrow {
    image: url(':/icons/upArrow_small');
}

QAbstractSpinBox::down-button {
    subcontrol-origin: border;
    subcontrol-position: bottom right;
    width: 21px;
    background: #e7e7e7;
    /* Remove this to get 3D rendered buttons */
    padding-right: 1px;
    padding-left: 5px;
    padding-bottom: 2px;
}

QAbstractSpinBox::down-arrow {
    image: url(':/icons/downArrow_small');
}


QValueComboBox {
    padding: 3px 5px 3px 5px;
    background: #b8b0b5;
    font-size: 12px;
    min-height: 25px;
    outline: 0;
    padding: 3px;
    border: 0px;
    color: #1d1b1c;
    background-color: #b8b0b5;
}

QValueComboBox:!focus {
    background-color: #b8b0b5;
    color: #1d1b1c;
}


/****************************************************************************************/

QHeaderView {
    /* Table Header */
    background-color: transparent;
    outline: 0;
}

QHeaderView::section {
    /* Table Header Sections */
    qproperty-alignment: center;
    background-color: #646163; /* for example on the tx page, where date, label, etc is. */
    color: #ffffff;
    min-height: 25px;
    font-weight: bold;
    font-size: 11px;
    border: 0;
    border-right: 1px solid #1d1b1c;
    padding-left: 5px;
    padding-right: 5px;
    padding-top: 2px;
    padding-bottom: 2px;
}

QHeaderView::down-arrow {
    image: url(':/icons/downArrow_small_white');
}

QHeaderView::up-arrow {
    image: url(':/icons/upArrow_small_white');
}

QHeaderView::section:last {
    border-right: 0;
}

.QScrollArea {
    background: #b8b0b5;
    border: 0;
}

.QTableView {
    /* Table - has to be selected as a class otherwise it throws off QCalendarWidget */
    background: transparent;
    border: 1px solid #1d1b1c;
}

QTableView::item {
    /* Table Item */
    background-color: #221f21;
    font-size: 12px;
    color: #ffffff;
}

QTableView::item:selected {
    /* Table Item Selected */
    background-color: #b8b0b5;
    color: #1d1b1c;
}

QTableWidget {
    /* Table Background */
    background-color: #ffffff;
    alternate-background-color: #1d1b1c;
    border: 1px solid #d6d6d6;
    color: #ffffff;
}

QTableWidget:focus,
QTableView,
QTableView:focus {
    /* Remove focus outline */
    outline: 0;
    background: transparent;
}

QTreeWidget {
    /* Tree Background */
    background-color: #1d1b1c;
    alternate-background-color: #f2f2f2;
    color: #1d1b1c;
}

QScrollBar {
    /* Scroll Bar */
}

QScrollBar:vertical {
    /* Vertical Scroll Bar Attributes */
    border: 0;
    background: #b8b0b5;
    width: 18px;
    margin: 18px 0px 18px 0px;
}

QScrollBar:horizontal {
    /* Horizontal Scroll Bar Attributes */
    border: 0;
    background: #b8b0b5;
    height: 18px;
    margin: 0px 18px 0px 18px;
}

QScrollBar::handle:vertical {
    /* Scroll Bar Slider - vertical */
    background: #1d1b1c;
    min-height: 10px;
}

QScrollBar::handle:vertical:pressed {
    /* Scroll Bar Slider - vertical */
    background: #1d1b1c;
    min-height: 10px;
}

QScrollBar::handle:horizontal {
    /* Scroll Bar Slider - horizontal */
    background: #1d1b1c;
    min-width: 10px;
}

QScrollBar::handle:horizontal:pressed {
    /* Scroll Bar Slider - horizontal */
    background: #1d1b1c;
    min-width: 10px;
}

QScrollBar::add-page,
QScrollBar::sub-page {
    /* Scroll Bar Background */
    background: #b8b0b5;
}

QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical,
QScrollBar::add-line:horizontal,
QScrollBar::sub-line:horizontal {
    /* Define Arrow Button Dimensions */
    background-color: #b8b0b5;
    border: 1px solid #f2f0f0;
    width: 16px;
    height: 16px;
}

QScrollBar::add-line:vertical:pressed,
QScrollBar::sub-line:vertical:pressed,
QScrollBar::add-line:horizontal:pressed,
QScrollBar::sub-line:horizontal:pressed {
    background-color: #e0e0e0;
}

QScrollBar::sub-line:vertical {
    /* Vertical - top button position */
    subcontrol-position: top;
    subcontrol-origin: margin;
}

QScrollBar::add-line:vertical {
    /* Vertical - bottom button position */
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal {
    /* Vertical - left button position */
    subcontrol-position: left;
    subcontrol-origin: margin;
}

QScrollBar::add-line:horizontal {
    /* Vertical - right button position */
    subcontrol-position: right;
    subcontrol-origin: margin;
}

QScrollBar:up-arrow,
QScrollBar:down-arrow,
QScrollBar:left-arrow,
QScrollBar:right-arrow {
    /* Arrows Icon */
    width: 10px;
    height: 10px;
}

QScrollBar:up-arrow {
    background-image: url(':/icons/upArrow_small');
}

QScrollBar:down-arrow {
    background-image: url(':/icons/downArrow_small');
}

QScrollBar:left-arrow {
    background-image: url(':/icons/leftArrow_small');
}

QScrollBar:right-arrow {
    background-image: url(':/icons/rightArrow_small');
}

QSlider::groove:horizontal {
    border: 1px solid #bbb;
    background: white;
    height: 6px;
    border-radius: 4px;
}

QSlider::sub-page:horizontal {
    /* background: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0, stop: 0 #B6A27E, stop: 1 #7b67a9); */
    background-color: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0, stop: 0 #221F21, stop: 1 #b8b0b5);
    border: 1px solid #1d1b1c;
    height: 10px;
    border-radius: 4px;
}

QSlider::add-page:horizontal {
    background: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 0, stop: 0 #221F21, stop: 1 #b8b0b5);
    border: 1px solid #1d1b1c;
    height: 10px;
    border-radius: 4px;
}

QSlider::handle:horizontal {
    background: qlineargradient(x1: 0, y1: 0, x2: 1, y2: 1, stop: 0 #221F21, stop: 1 #b8b0b5);
    border: 1px solid #1d1b1c;
    width: 18px;
    height: 18px;
    margin-top: -6px;
    margin-bottom: -6px;
    border-radius: 9px;
}

QSlider::handle:horizontal:hover {
    background-color: qradialgradient(cx: 0.5, cy: 0.5, fx: 0.5, fy: 0.5, radius: 0.7, stop: 0 #221F21, stop: 0.7 #b8b0b5);
    border: 2px solid #1d1b1c;
    width: 16px;
    height: 16px;
    margin-top: -6px;
    margin-bottom: -6px;
    border-radius: 8px;
}


