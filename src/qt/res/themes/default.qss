}

.QValidatedLineEdit:!focus {
    background-color: #b8b0b5;
    color: #1d1b1c;
}

.QLineEdit:!focus {
    background-color: #b8b0b5;
    color: #1d1b1c;
}

.QTextEdit:!focus {
    background-color: #b8b0b5;
    color: #1d1b1c;
}


/* .QValidatedLineEdit:checked, .QLineEdit:checked */

.QValidatedLineEdit:disabled,
.QLineEdit:disabled,
.QTextEdit:disabled {
    color: #1d1b1c;
