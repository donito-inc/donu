QToolBar {
    /* background-color: qlineargradient(y1: 0, y2: 1, stop: 0 #4a68b1, stop: 0.75 #4a68b1, stop: 0.99 #253458, stop: 1 #253458); */
    background-color: #1d1b1c;
    border: 0px solid #d6d6d6;
    padding: 1;
    margin: 0;
    width: 70px;
}


/* Left pane menu items */
QToolBar>QToolButton {
    Alignment: left;
    background-color: yellow;
    selection-background-color: transparent;
    border: 0px solid #d6d6d6;
    border-left: 0px;
    border-right: 0px;
    border-top: 0px;
    min-height: 2.5em;
    padding: 0em 0em;
    font-size: 11px;
    color: #ffffff;
    width: 80px;
    height: 60px;
}

QToolButton#overviewButton
{
    background-color: #1d1b1c;
    padding: 10px 0 10px;
    image: url(':/icons/overview');
}

QToolButton#overviewButton:checked,
QToolButton#overviewButton:hover
{
    background-color: #ffffff;
    padding: 10px 0 10px;
    image: url(':/icons/overview_active');
}

QToolButton#sendcoinsButton
{
    background-color: #1d1b1c;
    padding: 10px 0 10px;
    image: url(':/icons/send');
}

QToolButton#sendcoinsButton:checked,
QToolButton#sendcoinsButton:hover
{
    background-color: #ffffff;
    padding: 10px 0 10px;
    image: url(':/icons/send_active');
}

QToolButton#receivecoinsButton
{
    background-color: #1d1b1c;
    padding: 10px 0 10px;
    image: url(':/icons/receiving_addresses');
}

QToolButton#receivecoinsButton:checked,
QToolButton#receivecoinsButton:hover
{
    background-color: #ffffff;
    padding: 10px 0 10px;
    image: url(':/icons/receiving_addresses_active');
}

QToolButton#historyButton
{
    background-color: #1d1b1c;
    padding: 10px 0 10px;
    image: url(':/icons/history');
}

QToolButton#historyButton:checked,
QToolButton#historyButton:hover
{
    background-color: #ffffff;
    padding: 10px 0 10px;
    image: url(':/icons/history_active');
}

QToolButton#mintingButton
{
    background-color: #1d1b1c;
    padding: 10px 0 10px;
    image: url(':/icons/minting');
}

QToolButton#mintingButton:checked,
QToolButton#mintingButton:hover
{
    background-color: #ffffff;
    padding: 10px 0 10px;
    image: url(':/icons/minting_active');
}

QMessageBox {
    background-color: #ffffff;
    color: #1d1b1c;
}

QMessageBox QLabel {
    /* background-color: #ffffff; */
    color: #1d1b1c;
}

QProgressBar {
    color: #d6d6d6;
    border: 0px solid grey;
    border-radius: 5px;
    background-color: transparent;
    padding-left: 0px;
    padding-right: 0px;
}

QProgressBar::chunk {
    background-color: #8a8487;
    width: 20px;
}

QLabel#progressBarLabel {
    background-color: transparent;
    color: #ffffff;
