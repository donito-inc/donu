    padding-right: 5px;
}

QTabWidget {
    background-color: #ffffff;
}

QTabWidget::pane {
    background-color: #ffffff;
    border: 1px solid #1d1b1c;
}

QTabBar {
    background-color: #ffffff;
    color: #1d1b1c;
}

QTabBar::tab:hover:!selected {
    background-color: #ffffff;
}

QWidget {
    selection-background-color: #1d1b1c;
    /* Object highlight color */
    selection-color: #1d1b1c;
    outline: 0;
    /* Remove Annoying Focus Rectangle */
}

QGroupBox {
    background-color: #b8b0b5;
    color: #1d1b1c;
}

QToolTip {
    border-style: none;
    background-color: #1d1b1c;
    color: #ffffff;
}


